<div class="container container-custom" style="min-height:650px;">

    <% if flash[:error] %>
        <div class="alert alert-warning alert-dismissible" role="alert">
          <button type="button" class="close" data-dismiss="alert">
            <span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
          </button>
          <%= flash[:error] %>
        </div>
    <% end %>
    <% if notice %>
        <p class="alert alert-success alert-dismissible" role="alert" >
          <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
          <%= notice %>
        </p>
    <% end %>
    <% if alert %>
        <p class="alert alert-danger alert-dismissible" role="alert" ><button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button><%= alert %></p>
    <% end %>

<div class="conatiner container-custom" id="wrapper">

<br>

<div class="row">
	<div class="col-md-1"></div>
	<!-- <div id= "container" class="pie col-md-5">
	</div>
	<div id= "team_user" class="pie col-md-5">
	</div> -->
</div>
</div>
</div>
<script type="text/javascript">

	$(function () {
    var chart;
    var options = { videoId: 'cVD9ucODtpg', start: 3 };
    $('#wrapper').tubular(options);
    

    	
    	// Build the chart
        $('#container').highcharts({
            chart: {
                plotBackgroundColor: null,
                plotBorderWidth: null,
                plotShadow: false
            },
            title: {
                text: 'Team Coin Shares'
            },
            tooltip: {
        	    pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
            },
            plotOptions: {
                pie: {
                    allowPointSelect: true,
                    cursor: 'pointer',
                    dataLabels: {
                        enabled: false
                    },
                    showInLegend: true
                }
            },
            series: [{
                type: 'pie',
                name: 'Team Coins Shares',
                data: <%= raw @team_coins %>
        }]
        });

        $('#team_user').highcharts({
            chart: {
                plotBackgroundColor: null,
                plotBorderWidth: null,
                plotShadow: false
            },
            title: {
                text: 'Team Users Shares'
            },
            tooltip: {
        	    pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
            },
            plotOptions: {
                pie: {
                    allowPointSelect: true,
                    cursor: 'pointer',
                    dataLabels: {
                        enabled: false
                    },
                    showInLegend: true
                }
            },
            series: [{
                type: 'pie',
                name: 'Team Users Shares',
                data: <%= raw @team_users %>
        }]
        });
    
});
</script>
 